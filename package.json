{
  "name": "pets-manager",
  "version": "1.0.0",
  "description": "Pets manager for The Pets Project",
  "main": "build/index.js",
  "namespace": "tpp",
  "scripts": {
    "start": "node build/index.js",
    "build": "rm -rf build && ttsc --project tsconfig.build.json && tscpaths -p tsconfig.json -s ./src -o ./build",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "test": "jest --no-cache --runInBand",
    "test:watch": "jest --no-cache --runInBand --watch --forceExit --detectOpenHandles",
    "coverage": "npm test -- --coverage --no-cache --forceExit --detectOpenHandles",
    "coverage:watch": "npm test -- --coverage --no-cache --forceExit --detectOpenHandles --watch",
    "lint": "npx eslint '*/**/*.{js,ts}'",
    "lint:fix": "npx eslint '*/**/*.{js,ts}' --quiet --fix"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThePetsProject/pets-manager.git"
  },
  "author": "Javier Novoa",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.3.6",
    "newrelic": "^8.15.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-routemap": "^1.1.1",
    "@types/jest": "^27.5.1",
    "@types/jest-when": "^3.5.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.33",
    "@types/supertest": "^2.0.12",
    "@types/ts-nameof": "^4.2.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-mock-axios": "^4.6.1",
    "jest-mock-extended": "^2.0.6",
    "jest-when": "^3.5.1",
    "mockingoose": "^2.15.2",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-nameof": "^5.0.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "tscpaths": "^0.0.9",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.4"
  },
  "bugs": {
    "url": "https://github.com/ThePetsProject/pets-manager/issues"
  },
  "homepage": "https://github.com/ThePetsProject/pets-manager#readme"
}
