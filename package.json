{
  "name": "pets-manager",
  "version": "1.0.0",
  "description": "Pets manager for The Pets Project",
  "main": "build/index.js",
  "namespace": "tpp",
  "scripts": {
    "start": "node build/index.js",
    "build": "rm -rf build && ttsc --project tsconfig.build.json && tscpaths -p tsconfig.json -s ./src -o ./build",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "test": "jest --no-cache --runInBand",
    "test:watch": "jest --no-cache --runInBand --watch --forceExit --detectOpenHandles",
    "coverage": "npm test -- --coverage --no-cache --forceExit --detectOpenHandles",
    "coverage:watch": "npm test -- --coverage --no-cache --forceExit --detectOpenHandles --watch",
    "lint": "npx eslint '*/**/*.{js,ts}'",
    "lint:fix": "npx eslint '*/**/*.{js,ts}' --quiet --fix"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThePetsProject/pets-manager.git"
  },
  "author": "Javier Novoa",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.0",
    "newrelic": "^11.5.0",
    "ttsc": "^0.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/express-routemap": "^1.1.4",
    "@types/jest": "^29.5.8",
    "@types/jest-when": "^3.5.5",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@types/supertest": "^2.0.16",
    "@types/ts-nameof": "^4.2.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-mock-axios": "^4.7.3",
    "jest-mock-extended": "^3.0.5",
    "jest-when": "^3.6.0",
    "mockingoose": "^2.16.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-nameof": "^5.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.2.2"
  },
  "bugs": {
    "url": "https://github.com/ThePetsProject/pets-manager/issues"
  },
  "homepage": "https://github.com/ThePetsProject/pets-manager#readme"
}
